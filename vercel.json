{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/client/dist/$1" }
  ],
  "ignore": [
    "server/vite.ts",
    "scripts/**",
    "test-*/**",
    "test-*.js",
    "test-*.cjs",
    "test-*.sh",
    "test-legacy/**",
    "*.md",
    "*.txt",
    "*.sh",
    "*.cjs",
    "*.js",
    "!api/**",
    "!client/**",
    "!server/**",
    "!shared/**",
    "!prisma/**",
    "!package.json",
    "!yarn.lock",
    "!vercel.json"
  ]
}
